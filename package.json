{
  "name": "helix-snippets",
  "displayName": "Helix Snippets",
  "description": "Snippets for the helix gamemode in garrys mod.",
  "version": "1.0.3",
  "publisher": "Fedox",
  "icon": "images/helix-logo.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/Fedox-die-Ente/helix-snippets.git"
  },
  "keywords": [
    "Helix",
    "GMOD Helix",
    "Helix Snippets"
  ],
  "engines": {
    "vscode": "^1.63.0"
  },
  "categories": [
    "Snippets"
  ],
  "main": "./out/src/extension",
  "browser": "./out/src/extension",
  "contributes": {
    "snippets": [
      {
        "language": "glua",
        "path": "./snippets/blanks.json"
      },
      {
        "language": "lua",
        "path": "./snippets/blanks.json"
      },
      {
        "language": "glua",
        "path": "./snippets/classes.json"
      },
      {
        "language": "lua",
        "path": "./snippets/classes.json"
      },
      {
        "language": "glua",
        "path": "./snippets/hooks.json"
      },
      {
        "language": "lua",
        "path": "./snippets/hooks.json"
      }
    ],
    "commands": [
      {
        "command": "extension.stayClassy",
        "title": "GLUA: class to className"
      }
    ]
  },
  "activationEvents": [
    "onCommand:extension.stayClassy"
  ],
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "test": "node ./node_modules/@vscode/test-electron/out/runTest.js"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.32",
    "@types/node": "^6.0.40",
    "@vscode/test-electron": "^1.6.1",
    "mocha": "^8.0.1",
    "typescript": "^4.1.6",
    "@types/ws": "8.5.3"
  },
  "dependencies": {
    "@types/vscode": "^1.85.0"
  }
}
